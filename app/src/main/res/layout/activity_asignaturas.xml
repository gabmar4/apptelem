<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Asignaturas">

    <TextView
        android:id="@+id/media_co"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="Media no calculada"
        app:layout_constraintEnd_toEndOf="@+id/text_notas"
        app:layout_constraintStart_toStartOf="@+id/text_notas"
        app:layout_constraintTop_toBottomOf="@+id/media_cm" />

    <TextView
        android:id="@+id/media_cd"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="Media no calculada"
        app:layout_constraintEnd_toEndOf="@+id/text_notas"
        app:layout_constraintStart_toStartOf="@+id/text_notas"
        app:layout_constraintTop_toBottomOf="@+id/media_co" />

    <TextView
        android:id="@+id/media_cm"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="Media no calculada"
        app:layout_constraintEnd_toEndOf="@+id/text_notas"
        app:layout_constraintStart_toStartOf="@+id/text_notas"
        app:layout_constraintTop_toBottomOf="@+id/media_li" />

    <TextView
        android:id="@+id/media_li"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="Media no calculada"
        app:layout_constraintEnd_toEndOf="@+id/text_notas"
        app:layout_constraintStart_toStartOf="@+id/text_notas"
        app:layout_constraintTop_toBottomOf="@+id/media_ra" />

    <TextView
        android:id="@+id/media_ra"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="Media no calculada"
        app:layout_constraintEnd_toEndOf="@+id/text_notas"
        app:layout_constraintStart_toStartOf="@+id/text_notas"
        app:layout_constraintTop_toBottomOf="@+id/media_at" />

    <TextView
        android:id="@+id/texto_at"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:text="Aplicaciones Telemáticas"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/texto_asig"
        app:layout_constraintStart_toStartOf="@+id/texto_asig"
        app:layout_constraintTop_toBottomOf="@+id/texto_asig" />

    <TextView
        android:id="@+id/media_at"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:text="Media no calculada"
        app:layout_constraintEnd_toEndOf="@+id/text_notas"
        app:layout_constraintStart_toStartOf="@+id/text_notas"
        app:layout_constraintTop_toBottomOf="@+id/text_notas" />

    <TextView
        android:id="@+id/texto_ra"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="Radiocomunicaciones"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/texto_at"
        app:layout_constraintStart_toStartOf="@+id/texto_at"
        app:layout_constraintTop_toBottomOf="@+id/texto_at" />

    <TextView
        android:id="@+id/texto_li"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="Lineas"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/texto_ra"
        app:layout_constraintStart_toStartOf="@+id/texto_ra"
        app:layout_constraintTop_toBottomOf="@+id/texto_ra" />

    <TextView
        android:id="@+id/texto_cm"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="Comunicaciones Multimedia"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/texto_li"
        app:layout_constraintStart_toStartOf="@+id/texto_li"
        app:layout_constraintTop_toBottomOf="@+id/texto_li" />

    <TextView
        android:id="@+id/texto_co"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="Comunicaciones Ópticas"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/texto_cm"
        app:layout_constraintStart_toStartOf="@+id/texto_cm"
        app:layout_constraintTop_toBottomOf="@+id/texto_cm" />

    <TextView
        android:id="@+id/texto_cd"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="Comunicaciones Digitales"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/texto_co"
        app:layout_constraintStart_toStartOf="@+id/texto_co"
        app:layout_constraintTop_toBottomOf="@+id/texto_co" />

    <TextView
        android:id="@+id/texto_asig"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="48dp"
        android:layout_marginLeft="48dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="16dp"
        android:text="Asignaturas"
        android:textColor="#3F51B5"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toStartOf="@+id/text_notas"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/text_notas"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginLeft="24dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="24dp"
        android:layout_marginRight="24dp"
        android:text="Nota media obtenida"
        android:textColor="#3F51B5"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/texto_asig"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/text_error"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:textColor="#F80B0B"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/text_notas"
        app:layout_constraintHorizontal_bias="0.544"
        app:layout_constraintStart_toStartOf="@+id/texto_asig"
        app:layout_constraintTop_toBottomOf="@+id/media_cd" />
</android.support.constraint.ConstraintLayout>